{
  "name": "nearme",
  "version": "0.0.1",
  "description": "A web-based app to find locations of anything near me.",
  "main": "parser.js",
  "dependencies": {
    "axios": "^0.21.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsdom": "^16.6.0",
    "knex": "^0.95.6",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "eslint": "^7.32.0"
  },
  "scripts": {
    "start": "node rest_api.js",
    "dev": "nodemon rest_api.js",
    "parse-kfc": "node -e \"require('./parser_kfc.js')()\"",
    "parse-mcd": "node -e \"require('./parser_mcd.js')()\"",
    "parse-nfp": "node -e \"require('./parser_nfp.js')()\"",
    "db:create": "node create_db.js",
    "db:drop": "node drop_db.js",
    "db:setup": "knex migrate:latest",
    "db:reset": "knex migrate:rollback",
    "db:teardown": "npm run db:reset && npm run db:drop",
    "db:populate": "node run_parsers.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "html",
      "rest_api.js"
    ]
  },
  "author": "alicia michael",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/aliciatay-zls/NearMe.git"
  }
}
