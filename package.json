{
  "name": "nearme",
  "version": "0.0.1",
  "description": "A web-based app to find locations of anything near me.",
  "main": "parser.js",
  "dependencies": {
    "csv-writer": "^1.6.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsdom": "^16.6.0",
    "knex": "^0.95.6",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {},
  "scripts": {
    "dev": "nodemon rest_api.js",
    "parse-kfc": "node parser_kfc.js",
    "parse-mcd": "node parser_mcd.js",
    "parse-nfp": "node parser_nfp.js",
    "db:create": "node create_db.js",
    "db:drop": "node drop_db.js",
    "db:setup": "knex migrate:latest",
    "db:reset": "knex migrate:rollback",
    "db:teardown": "npm run db:reset && npm run db:drop",
    "db:populate": "npm run parse-kfc && npm run parse-mcd && npm run parse-nfp && npx knex seed:run",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "html",
      "rest_api.js"
    ]
  },
  "author": "alicia michael",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/aliciatay-zls/NearMe.git"
  }
}
